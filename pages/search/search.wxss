.container {
  padding: 20px;
}

input {
  width: 100%;
  height:100rpx;
  padding: 10px;
  margin-bottom: 10px;
  box-sizing: border-box;
}

button {
  width: 100%;
  padding: 10px;
  background-color: #1aad19;
  color: white;
  border: none;
  border-radius: 5px;
}

.result-list {
  margin-top: 20px;
}

.result-item {
  margin-bottom: 15px;
}

.title {
  font-size: 18px;
  font-weight: bold;
}

.url {
  color: #888;
  font-size: 14px;
  margin-top: 5px;
}
